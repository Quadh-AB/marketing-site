---
interface Props {
  name: string;
  linkedIn: string;
  github: string;
}

import { Github, Linkedin } from 'lucide-astro';
const { name, linkedIn, github } = Astro.props as Props;
---

<div>
  <img src="https://flowbite.s3.amazonaws.com/blocks/marketing-ui/avatars/bonnie-green.png" alt="" />
  <div>{name}</div>
  <div class="flex">
    <Github />
    <Linkedin />
  </div>
</div>
